{
  "name": "dominic_okapal_portfolio",
  "version": "1.0.0",
  "homepage": "https://okapaldominic.github.io",
  "description": "A portfolio site for Dominic Okapal",
  "main": "index.js",
  "dependencies": {
    "components": "^0.1.0",
    "copyfiles": "^2.1.1",
    "gh-pages": "^2.1.1",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-modal": "^3.10.1",
    "react-router-dom": "^5.1.2",
    "rimraf": "^2.7.1",
    "styled-components": "^4.4.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "parcel-bundler": "^1.12.4"
  },
  "scripts": {
    "build": "parcel build ./index.html",
    "start": "parcel ./index.html -p 1337",
    "deploygh": "gh-pages --dist dist --branch master",
    "copy": "copyfiles -u 1 src/examples/**/** dist && copyfiles 404.html dist",
    "deploy": "rimraf dist && npm run build && npm run copy && npm run deploygh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OkapalDominic/okapaldominic.github.io"
  },
